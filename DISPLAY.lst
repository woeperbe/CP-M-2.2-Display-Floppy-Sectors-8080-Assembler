02AE  32 4004           			STA		CNTLINES				; save it
02B1  CD 2C03           ALOOP:		CALL	HLOUT2					; Display address (in hex) with 2 spaces
02B4  16 10             			MVI		D,16					; 16 bytes / line
02B6  E5                			PUSH	H						; Save address
02B7  7E                ALP1:		MOV		A,M						; Get byte
02B8  CD FE02           			CALL	HPR						; Output in hex
02BB  CD 3203           			CALL	SPACE					; Space over
02BE  7A                			MOV		A,D						; Get count
02BF  FE 09             			CPI		9						; At boundary?
02C1  CC 3203           			CZ		SPACE					; Yes, extra space
02C4  7A                			MOV		A,D						; Get count
02C5  E6 07             			ANI		7						; Mask for low bits
02C7  FE 05             			CPI		5						; At boundary?
02C9  CC 3203           			CZ		SPACE					; Extra space
02CC  23                			INX		H						; Advance address
02CD  15                			DCR		D						; Reduce count
02CE  C2 B702           			JNZ		ALP1					; Do them all
02D1  16 04             			MVI		D,4						; # separating spaces
02D3  CD 3203           AL2:		CALL	SPACE					; Output a space
02D6  15                			DCR		D						; Reduce count
02D7  C2 D302           			JNZ		AL2						; And proceed
02DA  E1                			POP		H
02DB  16 10             			MVI		D,16					; 16 chars / display
02DD  7E                AL3:		MOV		A,M						; Get data byte
